ESP8266WebServer server(80);
DNSServer dnsServer;

#undef  USESD
#undef  USEAD
#define USEIO
#define USESERIAL

// Define I/O used by all peripherals.
#define TFTDC   4  // D/C select line to TFT
#define TFTCS   15 // Active LOW to TFT
#define TFTBL   16 // Active HIGH to turn on TFT backlight
#define TFTRST  0  // No pin assigned to TFT reset

// Create instances of our peripherals.
boolean alreadyConnected = false; // whether or not the client was connected previously
int frame = 0;

// TFT display based on ST7735 controller.
Adafruit_ST7735 tft = Adafruit_ST7735(TFTCS, TFTDC, TFTRST);

void setup() {
  
  pinMode(TFTDC, OUTPUT);
  pinMode(TFTCS, OUTPUT);  
  pinMode(TFTBL, OUTPUT);

  analogWrite(TFTBL, 150);  // TFT backlight
  
  tft.initR(INITR_BLACKTAB); // initialize a ST7735S chip, black tab
  tft.setRotation(1); // Landscape mode across the badge
  tft.fillScreen(ST7735_BLACK); // clear the screen

  tft.setTextColor(ST7735_WHITE, ST7735_BLACK); // White on black
  tft.setTextWrap(false); // Don't wrap text to next line
  tft.setTextSize(1); // small letters
  tft.setRotation(1); // horizontal display

  WiFi.softAP("HTTPMyBadger");
  
  dnsServer.start(53, "*", IPAddress(192,168,4,1));
  
  server.begin();

  server.on("/", [](){
    server.send(200,"text/html","<html><body bgcolor='#000' text='#fff'>Hello, BSides friend!.<br /><img src='/badger.gif'><br />By <a href='https://twitter.com/devec0'>ec0 (James Hebden)<a/>");
  });
  
  server.on("/badger.gif", [](){
    server.send_P(200, "image/gif", badger, sizeof(badger));
  });
  
  tft.println("SSID: HTTPMyBadger");
  tft.println("http://192.168.4.1");
  
}

void loop() {

  dnsServer.processNextRequest();
  server.handleClient();

  tft.fillRect(0, 16, 120, 120, ST7735_BLACK);
  switch (frame) {
    case 0:
      tft.drawBitmap(0, 16, badger_0, 120, 120, ST7735_WHITE);
      frame++;
      break;
    case 1:
      tft.drawBitmap(0, 16, badger_1, 120, 120, ST7735_WHITE);
      frame++;
      break;
    case 2:
      tft.drawBitmap(0, 16, badger_2, 120, 120, ST7735_WHITE);
      frame++;
      break;
    case 3:
      tft.drawBitmap(0, 16, badger_3, 120, 120, ST7735_WHITE);
      frame = 0;
      break;
    default: 
      frame = 0;
    break;
  }
  
  yield();
}
